@model _33.Models.QuickLink

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>QuickLink</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.HomeModelId, "HomeModel")
        </div>
        <div class="editor-field">
            @Html.DropDownList("HomeModelId", String.Empty)
            @Html.ValidationMessageFor(model => model.HomeModelId)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.LinkText)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.LinkText)
            @Html.ValidationMessageFor(model => model.LinkText)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ActionName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ActionName)
            @Html.ValidationMessageFor(model => model.ActionName)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ContollerName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ContollerName)
            @Html.ValidationMessageFor(model => model.ContollerName)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
